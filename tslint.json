{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "import-name": [false],
    "max-line-length": [true, 120],
    "variable-name": [true, "check-format", "allow-pascal-case", "allow-leading-underscore"],
    "no-unused-variable": [true, { "ignore-pattern": "^_" }]
  },
  "rulesDirectory": []
}
